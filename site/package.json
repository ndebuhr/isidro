{
  "name": "isidro-site",
  "version": "0.1.0",
  "description": "Isidro Chatbot System",
  "main": "index.js",
  "scripts": {
    "dev": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender",
    "dev:preview": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender --buildFuture --buildDrafts",
    "build": "exec-bin node_modules/.bin/hugo/hugo --gc --cleanDestinationDir --minify",
    "start:dev": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender --renderToDisk --cleanDestinationDir",
    "start:prod": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender --renderToDisk --cleanDestinationDir --minify --environment production",
    "npm-check": "npx npm-check -u",
    "postinstall": "hugo-installer --version otherDependencies.hugo --extended --destination node_modules/.bin/hugo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndebuhr/isidro.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ndebuhr/isidro/issues"
  },
  "homepage": "https://github.com/ndebuhr/isidro#readme",
  "devDependencies": {
    "npm-check": "^5.9.2"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "autoprefixer": "^10.4.7",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.2",
    "exec-bin": "^1.0.0",
    "gsap": "^3.10.4",
    "hugo-installer": "^3.1.0",
    "lazysizes": "^5.3.2",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0"
  },
  "otherDependencies": {
    "hugo": "0.99.0"
  }
}
